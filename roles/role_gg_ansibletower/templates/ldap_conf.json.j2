{
    "AUTH_LDAP_SERVER_URI": "ldaps://{{ ldap_servers[0] }}:636",
    "AUTH_LDAP_BIND_DN": "{{ tower_ldap_bind_user }}",
    "AUTH_LDAP_BIND_PASSWORD": "{{ tower_ldap_bind_pass }}",
    "AUTH_LDAP_START_TLS": false,
    "AUTH_LDAP_CONNECTION_OPTIONS": {
        "OPT_NETWORK_TIMEOUT": 30,
        "OPT_REFERRALS": 0
    },
    "AUTH_LDAP_USER_SEARCH": [
        "{{ ldap_user_dn }}",
        "SCOPE_SUBTREE",
        "{{ ldap_user_filter }}"
    ],
    "AUTH_LDAP_USER_DN_TEMPLATE": "uid=%(user)s,cn=users,cn=accounts,dc=gatwards,dc=org",
    "AUTH_LDAP_USER_ATTR_MAP": {
        "first_name": "givenName",
        "last_name": "sn",
        "email": "mail"
    },
    "AUTH_LDAP_GROUP_SEARCH": [
        "{{ ldap_group_dn }}",
        "SCOPE_SUBTREE",
        "{{ ldap_group_filter }}"
    ],
    "AUTH_LDAP_GROUP_TYPE": "GroupOfNamesType",
    "AUTH_LDAP_REQUIRE_GROUP": "",
    "AUTH_LDAP_DENY_GROUP": null,
    "AUTH_LDAP_USER_FLAGS_BY_GROUP": {
    },
    "AUTH_LDAP_ORGANIZATION_MAP": {
        "Default": {
            "admins": "{{ tower_admins_group }}",
            "remove_admins": true
        }
    },
    "AUTH_LDAP_TEAM_MAP": {
    }
}
